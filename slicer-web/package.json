{
  "name": "slicer-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm exec next lint && pnpm exec prettier -c \"{app,components,lib,modules,workers}/**/*.{ts,tsx}\" \"tests/**/*.ts?(x)\" \"styles/**/*.css\"",
    "lint:staged": "pnpm exec lint-staged",
    "format": "pnpm exec prettier --write \"{app,components,lib,modules,workers}/**/*.{ts,tsx}\" \"tests/**/*.ts?(x)\" \"styles/**/*.css\"",
    "typecheck": "pnpm exec tsc --project tsconfig.app.json --noEmit",
    "test": "pnpm exec vitest run",
    "test:watch": "pnpm exec vitest watch",
    "e2e": "pnpm exec playwright test",
    "coverage": "pnpm exec vitest run --coverage",
    "prepare": "husky",
    "commitlint": "pnpm exec commitlint",
    "release": "release-please release"
  },
  "dependencies": {
    "comlink": "^4.4.1",
    "dexie": "^4.0.8",
    "fflate": "^0.8.2",
    "jspdf": "^2.5.2",
    "next": "15.5.4",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.170.0",
    "three-stdlib": "^2.36.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.17.10",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.4",
    "esbuild": "^0.25.10",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.1.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.13",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "release-please": "^16.14.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "@types/three": "^0.170.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm exec eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "pnpm exec prettier --write"
    ]
  }
}
